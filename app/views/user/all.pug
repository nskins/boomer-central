extends ../layout

block content
  div(class="container")
    div(class="col-md-4")
    div(class="col-md-4")
      hr
      for user in users
        p(class="pull-right") Favorite trick: #{user.favoriteTrick}
        p
          a(href='/users/' + user.username) #[strong #{user.username}]
        hr
    div(class="col-md-4")

